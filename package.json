{
    "name": "osu-api-v2",
    "version": "0.1.0",
    "description": "An easy way to use the osu!api v2",
    "main": "lib/index.mjs",
    "types": "lib/index.d.mts",
    "type": "module",
    "directories": {
        "lib": "./lib"
    },
    "devDependencies": {
        "@types/chai": "^4.3.4",
        "@types/mocha": "^10.0.1",
        "@types/node": "^18.16.0",
        "@typescript-eslint/eslint-plugin": "^5.59.0",
        "@typescript-eslint/parser": "^5.59.0",
        "chai": "^4.3.7",
        "eslint": "^8.39.0",
        "eslint-config-prettier": "^8.8.0",
        "eslint-plugin-jsdoc": "^43.0.7",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-typescript-sort-keys": "^2.3.0",
        "isomorphic-fetch": "^3.0.0",
        "madge": "^6.0.0",
        "mocha": "^10.2.0",
        "moment": "^2.29.4",
        "node-html-parser": "^6.1.5",
        "nyc": "^15.1.0",
        "prettier": "^2.8.8",
        "rimraf": "^5.0.0",
        "ts-node": "^10.9.1",
        "ts-unused-exports": "^9.0.4",
        "typedoc": "^0.24.5",
        "typedoc-umlclass": "^0.7.1",
        "typescript": "^5.0.4"
    },
    "scripts": {
        "build": "tsc",
        "build:check": "tsc --noEmit",
        "create:osuApiTypes": "ts-node --project tsconfig.eslint.json --esm scripts/createOsuApiTypes.mts",
        "docs": "typedoc --tsconfig tsconfig.json",
        "docs:createPagesExampleOutput": "ts-node --project tsconfig.json --esm pages/create_pages_example_output.mts",
        "format": "prettier --check examples src test",
        "format:fix": "prettier --write examples pages scripts src test",
        "lint": "eslint pages scripts src test",
        "lint:circular": "madge --circular lib/index.mjs",
        "lint:exports": "ts-unused-exports tsconfig.json --showLineNumber",
        "lint:fix": "eslint --fix pages scripts src test",
        "prebuild": "rimraf lib",
        "test": "mocha -- test/**/**/*.test.mts test/**/*.test.mts test/*.test.mts",
        "test:no-refresh-token": "mocha --invert --fgrep refresh-token -- test/**/**/*.test.mts test/**/*.test.mts test/*.test.mts",
        "test:coverage": "nyc --reporter=html --reporter=text mocha -- test/**/**/*.test.mts test/**/*.test.mts test/*.test.mts",
        "test:coverage-no-refresh-token": "nyc --reporter=html --reporter=text mocha --invert --fgrep refresh-token -- test/**/**/*.test.mts test/**/*.test.mts test/*.test.mts",
        "test:no-osu-api-v2": "mocha --invert --fgrep osu-api-v2 -- test/**/**/*.test.mts test/**/*.test.mts test/*.test.mts"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/AnonymerNiklasistanonym/osu-api-v2.git"
    },
    "keywords": [
        "api",
        "osu",
        "async",
        "promise",
        "typescript"
    ],
    "author": {
        "name": "AnonymerNiklasistanonym"
    },
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/AnonymerNiklasistanonym/osu-api-v2/issues"
    },
    "homepage": "https://github.com/AnonymerNiklasistanonym/osu-api-v2#readme",
    "dependencies": {
        "@types/node-fetch": "^2.6.3",
        "node-fetch": "^3.3.1"
    }
}
