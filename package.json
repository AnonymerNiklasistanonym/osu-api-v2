{
    "name": "osu-api-v2",
    "version": "0.0.6",
    "description": "An easy way to use the osu!api v2",
    "main": "lib/index.js",
    "types": "lib/index.d.ts",
    "directories": {
        "lib": "./lib"
    },
    "devDependencies": {
        "@types/chai": "^4.2.21",
        "@types/mocha": "^9.0.0",
        "@types/node": "^16.6.1",
        "@types/node-fetch": "^2.5.12",
        "@typescript-eslint/eslint-plugin": "^4.29.1",
        "@typescript-eslint/parser": "^4.29.1",
        "chai": "^4.3.4",
        "eslint": "^7.32.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-prettier": "^3.4.0",
        "mocha": "^9.0.3",
        "moment": "^2.29.1",
        "prettier": "^2.3.2",
        "ts-node": "^10.2.0",
        "typescript": "^4.3.5"
    },
    "scripts": {
        "prepare": "tsc",
        "build": "tsc",
        "build:check": "tsc --noEmit",
        "lint": "eslint src test",
        "lint-fix": "eslint --fix src test",
        "format": "prettier --check src test .prettierrc.js .eslintrc.js",
        "format-fix": "prettier --write src test .prettierrc.js .eslintrc.js",
        "test": "mocha --require ts-node/register test/**/**/*.test.ts test/**/*.test.ts test/*.test.ts",
        "test-without-osu-api-v2": "mocha --require ts-node/register --invert --grep ^osu-api-v2 test/**/**/*.test.ts test/**/*.test.ts test/*.test.ts"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/AnonymerNiklasistanonym/osu-api-v2.git"
    },
    "keywords": [
        "api",
        "osu",
        "async",
        "promise",
        "typescript"
    ],
    "author": {
        "name": "AnonymerNiklasistanonym"
    },
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/AnonymerNiklasistanonym/osu-api-v2/issues"
    },
    "homepage": "https://github.com/AnonymerNiklasistanonym/osu-api-v2#readme",
    "dependencies": {
        "node-fetch": "^2.6.1"
    }
}
